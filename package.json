    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.84.0",
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.38",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.0",
        "typescript": "^5.3.2",
        "vite": "^5.0.0",
        "vite-plugin-singlefile": "^0.13.5",
        "esbuild": "^0.19.0"
    }
}